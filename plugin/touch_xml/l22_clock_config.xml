	<data>	{Generic clock(Peripherals)| Source | Frequency},
					{GCLK0(CPU)|OSC16M| 16MHz}, 
					{GCLK1(PTC, UART)|OSC16M(DIV 4)|4MHz},
					{Timer(RTC)|OSCULP32K|32.768KHz}
	</data>
	<data>	{Wait states},
					{NVM states| 2}
	</data>
