<?xml version="1.0"?>
<!-- 
Note 1: For each new clock symbol increase the index id : 
	    Example: index = "0_0" symbol="abc" value="12"   // different symbol 
				 index = "1_0" symbol="def" value="2"    // different symbol
				 index = "2_0" symbol="def" value="5"    // different symbol
				 ...
Note 2: For same same clock symbol different value need to update means :-> increment the count value in each index".
		Example: index = "0_0" symbol="abc" value="12"   // same symbol
				 index = "0_1" symbol="abc" value="18"   // same symbol
				 index = "0_2" symbol="abc" value="25"   // same symbol
				 ...
Note 3: condition  name can be any thing. Java update clock symbols based on condition index id.
-->

<clock_settings>
	<all_clock_symbols_list>
		<clock index = "0_0" symbol="GCLK_1_DIV" value="12" name="Generic Clock Generator 1 (GCLK) Configuration" />
		<clock index = "0_1" symbol="GCLK_1_DIV" value="14" name="Generic Clock Generator 1 (GCLK) Configuration" />
		<clock index = "0_2" symbol="GCLK_1_DIV" value="24" name="Generic Clock Generator 1 (GCLK) Configuration" />
		<clock index = "1_0" symbol="GCLK_2_DIV" value="12" name="Generic Clock Generator 2 (GCLK) Configuration" />
		<clock index = "2_0" symbol="GCLK_3_DIV" value="12" name="Generic Clock Generator 3 (GCLK) Configuration" />
		<clock index = "3_0" symbol="GCLK_4_DIV" value="12" name="Generic Clock Generator 4 (GCLK) Configuration" />
		<clock index = "4_0" symbol="GCLK_5_DIV" value="12" name="Generic Clock Generator 5 (GCLK) Configuration" />
		<clock index = "5_0" symbol="GCLK_6_DIV" value="12" name="Generic Clock Generator 6 (GCLK) Configuration" />
		<clock index = "6_0" symbol="GCLK_7_DIV" value="12" name="Generic Clock Generator 7 (GCLK) Configuration" />
		<clock index = "7_0" symbol="GCLK_8_DIV" value="12" name="Generic Clock Generator 8 (GCLK) Configuration" />
	</all_clock_symbols_list>
	<all_conditions_list>
		<condition  condition_id ="0" touch_symbol="XYZ" name="sdfs"> 
			<touch_value touch_check_value="true" clock_update_index_list="6_0,7_0"/>
		</condition>
		<condition  condition_id ="1" touch_symbol="ABC" name="abcd"> 
			<touch_value touch_check_value="32" clock_update_index_list="0_0"/>
			<touch_value touch_check_value="12" clock_update_index_list="1_0,0_2"/>
		</condition>
	</all_conditions_list>
	<common>
		<default clock_update_index_list="0_0,1_0,2_0,3_0"/>
		<condition eligable_conditions="0"/>
	</common>
	<device name="ATSAMD21G15A" />
		<default clock_update_index_list="0_0,1_0,2_0,3_0" />
		<condition eligable_conditions="0,1"/>
	</device>
	<device name="ATSAMD20G15A" />
		<default clock_update_index_list="0_0,1_0,2_0,3_0,4_0" />
		<condition eligable_conditions="1"
	</device>
</clock_settings>
